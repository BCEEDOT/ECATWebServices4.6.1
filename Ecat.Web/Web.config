  <system.webServer>
    <httpErrors errorMode="Detailed" />
    <validation validateIntegratedModeConfiguration="false" />
    <rewrite>
      <rules>
        <rule name="EcatAngularStartup" stopProcessing="true">
          <match url="^$" />
          <action type="Rewrite" url="app" />
        </rule>
        <rule name="EcatAngularSecure" stopProcessing="true">
          <match url=".*" />
          <conditions logicalGrouping="MatchAll">
            <add input="{REQUEST_FILENAME}" matchType="IsFile" negate="true" />
            <add input="{REQUEST_FILENAME}" matchType="IsDirectory" negate="true" />
            <add input="{REQUEST_URI}" pattern="breeze\/" negate="true" ignoreCase="true" />
            <add input="{REQUEST_URI}" pattern="api\/" negate="true" ignoreCase="true" />
            <!--add input="{REQUEST_URI}" pattern="lti\/(Secure|Ping)" negate="true" ignoreCase="true" /-->
            <add input="{REQUEST_URI}" pattern="lti\/(LtiEntry)" negate="true" ignoreCase="true" />
            <add input="{REQUEST_URI}" pattern="canvasauth\/(TokenRequest)" negate="true" ignoreCase="true" />
            <add input="{REQUEST_URI}" pattern="connect\/token" negate="true" ignoreCase="true" />
            <add input="{REQUEST_URI}" pattern="favicon.ico" negate="true" ignoreCase="true" />
            <add input="{REQUEST_URI}" pattern="elmah" negate="true" ignoreCase="true" />
            <add input="{REQUEST_URI}" pattern="ecat\S*(\.\w+)$" negate="true" ignoreCase="true" />
          </conditions>
          <action type="Rewrite" url="app" />
        </rule>
      </rules>
    </rewrite>
    <handlers>
      
      
      
      
      <add name="ELMAH" verb="POST,GET,HEAD" path="elmah.axd" type="Elmah.ErrorLogPageFactory, Elmah" preCondition="integratedMode" />
    <remove name="ExtensionlessUrlHandler-Integrated-4.0" /><remove name="OPTIONSVerbHandler" /><remove name="TRACEVerbHandler" /><add name="ExtensionlessUrlHandler-Integrated-4.0" path="*." verb="*" type="System.Web.Handlers.TransferRequestHandler" preCondition="integratedMode,runtimeVersionv4.0" /></handlers>
    
    <staticContent>
      <mimeMap fileExtension="woff" mimeType="application/font-woff" />
      <mimeMap fileExtension="woff2" mimeType="application/font-woff" />
    </staticContent>
    
    <modules>
      <add name="ErrorLog" type="Elmah.ErrorLogModule, Elmah" preCondition="managedHandler" />
      <add name="ErrorMail" type="Elmah.ErrorMailModule, Elmah" preCondition="managedHandler" />
      <add name="ErrorFilter" type="Elmah.ErrorFilterModule, Elmah" preCondition="managedHandler" />
    </modules>
    
  </system.webServer>
  <elmah>
